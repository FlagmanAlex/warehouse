{
  "name": "warehouse",
  "private": true,
  "version": "1.0.0",
  "description": "Система складсколго учета",
  "workspaces": [
    "client",
    "server",
    "interfaces",
    "web",
    "tests"
  ],
  "main": "index.js",
  "scripts": {
    "server": "cd server && npm run dev",
    "client": "cd client && npm run client",
    "web": "npm run dev --workspace web",
    "web:build": "npm run build --workspace web",
    "import": "cd server && npm run import",
    "sinc": "cd server && npm run sinc",
    "restruct": "cd server && npm run restruct",
    "test:server": "npm run test --workspace server",
    "test:web": "npm run test --workspace web",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlagmanAlex/warehouse.git"
  },
  "keywords": [],
